#
# Check for the std::abs(...) overloads.
#
# GreenHills <cmath> defines ::abs(float), ::abs(double) and
# ::abs(long double), but does not place them into the std namespace when
# targeting mercury (when _MC_EXEC is defined).

# First check if std::abs handles float and double:
{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for std::abs(float), std::abs(double), and std::abs(long double)." >&5
$as_echo_n "checking for std::abs(float), std::abs(double), and std::abs(long double).... " >&6; }
have_abs_float="no"
cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */

#include <cmath>

int main(int, char **)
{
  float f1 = 1.f;
  f1 = std::abs(f1);
  (void)f1;
  double d1 = 1.0;
  d1 = std::abs(d1);
  (void)d1;
  long double l1 = 1.0;
  l1 = std::abs(l1);
  (void)l1;
}

_ACEOF
if ac_fn_cxx_try_compile "$LINENO"; then :
  have_abs_float="std"
 { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
